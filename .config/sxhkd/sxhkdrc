super + Return
  kitty 

super + Escape
  bspc wm -r &
  pkill -USR1 -x sxhkd

super + q
  bspc node -c

super + e
  rofi -show drun

super + Left
  bspc node -f west

super + Right
  bspc node -f east

super + Up
  bspc node -f north

super + Down
  bspc node -f south

super + f
  bspc node -t ~floating

super + w
  ~/.config/bspwm/wall.sh

super + x
  flameshot gui

super + b
  pgrep -x polybar && pkill -x polybar || polybar main &

F11
    bspc node -t ~fullscreen
